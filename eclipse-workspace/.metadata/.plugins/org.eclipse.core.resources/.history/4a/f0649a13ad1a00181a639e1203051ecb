
public class Homework4 {

	public static void main(String[] args) {
		
		int total = 1000;
		int MAX = 1000;
		int sum = 5000;
		
		if (total == MAX)
		{
		    if (total < sum)
		    System.out.println("total == MAX and < sum");
		    else
		    	System.out.println("total == MAX and > sum");
		}
		else
		    System.out.println("total is not equal to MAX");
	}

}
