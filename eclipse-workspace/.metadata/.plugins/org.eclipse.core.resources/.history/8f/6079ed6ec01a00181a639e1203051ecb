public class Homework4
{
    public static void main (String[] args)
    {
        int min = 0, max = 10;
        
        for (int rows = max; rows > min; rows--)
        {
            
            for (int star = min; star <= max; star++)
            {
                System.out.print("*");
            }
            System.out.println("");
        }
    }
}