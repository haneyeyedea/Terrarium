public class Homework4
{
    public static void main (String[] args)
    {
        int min = 0, max = 10;
        
        for (int rows = min; rows < max; rows++)
        {
            for (int space = max; space > min; space--)
            {
                System.out.print(" ");
            }
            
            for (int star = min; star < max; star++)
            {
                System.out.print("*");
            }
            System.out.println("");
        }
    }
}