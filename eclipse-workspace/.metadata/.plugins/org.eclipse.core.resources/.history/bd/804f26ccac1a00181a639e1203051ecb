
public class Homework4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int total = 80;
		int MAX = 1000;
		int sum = 14;
		
		if (total == MAX)
		{
		    if (total < sum)
		    System.out.println("total == MAX and < sum");
		}
		else
		    System.out.println("total is not equal to MAX");
	}

}
